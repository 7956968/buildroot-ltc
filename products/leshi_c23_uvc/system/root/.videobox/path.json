{
    "isp": {
        "ipu": "v2500",
        "args": {
            "mode": 4,
            "flip": 2,
            "name": "OV683Dual",
            "config": "/root/.ispddk/config-normal.txt"
        },
        "port": {
            "out": {
                "w": 1920,
                "h": 960,
                "pixel_format": "nv12",
                "fps": 30,
                "bind": {
                    "encavc0": "frame",
                    "jpeg": "in"
                }
            },
            "cap": {
                "w": 3008,
                "h": 1504,
                "pip_x": 0,
                "pip_y": 0,
                "pip_w": 3008,
                "pip_h": 1504,
                "bind": {"jpegfull":"in"}
            }
        }

    },

    "encavc0": { "ipu": "vencoder",
        "args": {
            "encode_type": "h264",
            "framebase": 1,
            "framerate": 30
        }
    },
    "jpeg": { "ipu": "h1jpeg", "args": { "mode": "trigger"}},
    "jpegfull": { "ipu": "h1jpeg", "args": { "mode": "trigger"}}
}
