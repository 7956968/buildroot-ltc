prefix=/usr
exec_prefix=/usr
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name:  hlibh1v6
Description: hlibh1v6  infotm
Version: 1.0.0
Requires: 
Libs: -L${libdir} -lh1v6
Libs.private: -lm -ldl -lpthread -lrt
# -I${includedir}/hlibh1v6 below is just for backward compatibility
# (it was set so mistakely in the older version)
Cflags:  -I${includedir}/hlibh1v6
