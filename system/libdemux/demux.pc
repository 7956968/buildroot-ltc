prefix=/usr
exec_prefix=/usr
libdir=${exec_prefix}/lib
includedir=${prefix}/include/qsdk

Name: demux
Description: libdemux  infotm
Version: 1.0.0
Requires:
Libs: -L${libdir} -ldemux -lavformat
Libs.private: -lm -ldl -lpthread
# -I${includedir}/alsa below is just for backward compatibility
# (it was set so mistakely in the older version)
Cflags:  -I${includedir} -I${prefix}/local/include/
