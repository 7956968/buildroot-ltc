prefix=/usr
exec_prefix=/usr
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name:  hlibg2v1
Description: hlibg2v1  infotm
Version: 1.0.0
Requires: 
Libs: -L${libdir} -lg2v1
Libs.private: -lm -ldl -lpthread -lrt
# -I${includedir}/hlibg2v1 below is just for backward compatibility
# (it was set so mistakely in the older version)
Cflags:  -I${includedir}/hlibg2v1
