prefix=/usr
exec_prefix=/usr
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name:  hlibh2v4
Description: hlibh2v4  infotm
Version: 1.0.0
Requires:
Libs: -L${libdir} -lh2v4
Libs.private: -lm -ldl -lpthread -lrt
# -I${includedir}/hlibh2v1 below is just for backward compatibility
# (it was set so mistakely in the older version)
Cflags:  -I${includedir}/hlibh2v4
