{
	"v4l2": {
		"ipu": "v4l2",
		"args": {
			"driver":"imapx_camif",
			"model": "gc0308_demo",
			"pixfmt": "nv12"
		},
		"port": {
			"out": {
				"w": 640,
				"h": 480,
				"pixel": "nv12",
				"bind": { "display": "osd0", "h1264":"frame" }
			}
		}
	},

	"h1264":{
		"ipu": "vencoder",
		"args": {
			"encode_type": "h264"
		},
		"port":{
			"stream":{
				"bind":{ "filesink":"in" }
			}
		}
	},

	"filesink":{
		"ipu":"filesink",
		"args":{
			"data_path":"/tmp/h264Test.h264"
		}
	},

	"display": { "ipu": "ids"}
}
