prefix=/usr
exec_prefix=/usr
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name:  hlibjenc
Description: hlibjenc  infotm
Version: 1.0.0
Requires:
Libs: -L${libdir} -ljenc
Libs.private: -lm -ldl -lpthread -lrt
# -I${includedir}/hlibjenc below is just for backward compatibility
# (it was set so mistakely in the older version)
Cflags:  -I${includedir}/hlibjenc
